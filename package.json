	"devDependencies": {
		"@babel/core": "^7.0.0-beta.54",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
		"@babel/preset-env": "^7.0.0-beta.54",
		"babel-core": "^7.0.0-0",
		"babel-jest": "^23.4.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.1.0",
		"eslint-config-airbnb-base": "^13.0.0",
		"eslint-plugin-import": "^2.13.0",
		"jest": "^23.4.1",
		"rollup": "^0.63.2",
		"rollup-plugin-babel": "^4.0.0-beta.7",
		"rollup-plugin-commonjs": "^9.1.3",
		"rollup-plugin-json": "^3.0.0",
		"rollup-plugin-node-resolve": "^3.3.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.2",
		"debug": "^3.1.0",
		"form-data": "^2.3.2",
		"middleware-io": "^0.0.6",
		"node-fetch": "^2.1.2",
		"sandwich-stream": "^1.0.0",
		"tough-cookie": "^2.4.3",
		"ws": "^5.2.2"
	},
	"scripts": {
		"prepare": "npm run rollup:build && npm run test",
		"rollup:build": "cross-env NODE_ENV=production rollup -c",
		"rollup:watch": "npm run rollup:build -- -w",
		"test": "npm run test:jest && npm run test:eslint",
		"test:jest": "cross-env BABEL_ENV=test jest --config jest.config.json --no-cache",
		"test:eslint": "eslint ./src --ext .mjs,.js --ignore-path .gitignore ."
	}
